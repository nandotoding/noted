	</parent>
	<groupId>com.nandotoding</groupId>
	<artifactId>noted</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>noted</name>
	<description>noted! application</description>
	<url/>
	<licenses>
		<license/>
